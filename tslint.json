{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "interface-name": false,
    "prettier": true
  }
}
